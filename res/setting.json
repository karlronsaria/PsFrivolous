{
    "ActionList": {
        "FilePattern": "/note/todo/*.md",
        "TextColor": "Yellow",
        "NumberColor": "DarkGray",
        "Indent": 3,
        "DateFormat": "yyyy_MM_dd",
        "MarkdownBranchPattern": "^\\s*(\\d+\\.|-) "
    },
    "Prompt": {
        "ApplicationName": "PS",
        "TimeFormat": "HHmm",
        "DateTimeFormat": "yyyy_MM_dd [HH:mm:ss]",
        "Color": {
            "Application": "White",
            "Date": "DarkGreen",
            "User": "Yellow",
            "Domain": "Magenta"
        }
    },
    "Text": {
        "DefaultFontMap": "AnsiShadow"
    }
}
